            "usage": "cortx cluster start [-h] [--all | --server] [--json]",
            "class": "ha.cli.exec.clusterExecutor.ClusterStartExecutor"
        },
        "stop": {
            "usage": "cortx cluster stop [-h] [--all | --server] [--json]",
            "class": "ha.cli.exec.clusterExecutor.ClusterStopExecutor"
        },
        "restart": {
            "usage": "cortx cluster restart",
            "class": "ha.cli.exec.clusterExecutor.ClusterRestartExecutor"
        },
        "standby": {
            "usage": "cortx cluster standby [--json]",
            "class": "ha.cli.exec.clusterExecutor.ClusterStandbyExecutor"
